def install_pillow():
    if Image is None:
        install = askyesno(
            'Install PIL', 'You must install PIL library to use the function.\nPress Yes to install and No to cancel.')
        if install:
            errorlevel = os.system('pip.exe install pillow')
            if errorlevel != 0:
                showerror('Install PIL', 'Failed to install PIL library!')
            else:
                os.popen(f'"{__file__}"')
                sys.exit(0)


def install_pyperclip():
    if pyperclip is None:
        install = askyesno(
            'Install pyperclip', 'You must install pyperclip library to use the function.\nPress Yes to install and No to cancel.')
        if install:
            errorlevel = os.system('pip.exe install pyperclip')
            if errorlevel != 0:
                showerror('Install pyperclip',
                          'Failed to install pyperclip library!')
            else:
                os.popen(f'"{__file__}"')
                sys.exit(0)


def copy(text):
    if pyperclip is None:
        install_pyperclip()
        return
    pyperclip.copy(text)


