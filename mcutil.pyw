        self.clear_vars()
        self.toggle_buttons(False)
        try:
            username = self.var_name.get().strip()
            if RE_USERNAME.match(username):
                cape_image = request_bytes_http('s.optifine.net',
                                                f'http://s.optifine.net/capes/{username}.png')
                bytes_image = BytesIO(cape_image)
                image = Image.open(bytes_image)
                image = image.convert('RGB')
                image = image.resize((92, 44), resample=Image.NEAREST)
                image_banner = image.crop((2, 2, 22, 34))
                color_top = ''.join(
                    map('%02X'.__mod__, image.getpixel((22, 2))))
                color_bottom = ''.join(
                    map('%02X'.__mod__, image.getpixel((22, 33))))
                banner_url = None
                try:
                    banner_url = request_banner(username)
                except Exception:
                    banner_url = None
                if banner_url is None:
                    of_text = ''.join(
                        map('%02X'.__mod__, image.getpixel((6, 10))))
                    of_shadow = ''.join(
                        map('%02X'.__mod__, image.getpixel((6, 12))))
                    default_key = color_top + color_bottom + of_text + of_shadow
                    design = MAP_DESIGN.get(default_key, None)
                    if design is None:
                        self.var_design.set('Custom...')
                    else:
                        self.var_design.set(design)
                    self.toggle_buttons(True)
                    self.label_value1['text'] = 'Top:'
                    self.label_value2['text'] = 'Bottom:'
                    self.label_value3['text'] = 'Text:'
                    self.label_value4['text'] = 'Shadow:'
                    self.var_value1.set(color_top)
                    self.var_value2.set(color_bottom)
                    self.var_value3.set(of_text)
                    self.var_value4.set(of_shadow)
                else:
                    banner_align = 'Unknown'
                    banner_bytes = image_banner.tobytes()
                    for align_id, align_name in MAP_ALIGN.items():
                        match_image = request_bytes_http(
                            'optifine.net', f'http://optifine.net/showBanner?format={banner_url}&valign={align_id}')
                        bytes_match = BytesIO(match_image)
                        image_match = Image.open(bytes_match)
                        image_match = image_match.convert('RGB')
                        image_match = image_match.resize(
                            (92, 44), resample=Image.NEAREST)
                        image_match = image_match.crop((2, 2, 22, 34))
                        if image_match.tobytes() == banner_bytes:
                            banner_align = align_name
                            break
                    self.var_design.set('Banner...')
                    self.toggle_buttons(True)
                    self.label_value1['text'] = 'URL:'
                    self.label_value2['text'] = 'Align:'
                    self.label_value3['text'] = 'Top:'
                    self.label_value4['text'] = 'Bottom:'
                    self.var_value1.set(banner_url)
                    self.var_value2.set(banner_align)
                    self.var_value3.set(color_top)
                    self.var_value4.set(color_bottom)
                self.title('OptiFine Cape')
            else:
                self.var_design.set('Invalid name')
                self.title('OptiFine Cape (Failed)')
        except Exception:
            self.var_design.set('Not exist')
            self.title('OptiFine Cape (Failed)')

    def clear_vars(self):
        self.var_design.set('')
        self.var_value1.set('')
        self.label_value1['text'] = 'Value1:'
        self.var_value2.set('')
        self.label_value2['text'] = 'Value2:'
        self.var_value3.set('')
        self.label_value3['text'] = 'Value3:'
        self.var_value4.set('')
        self.label_value4['text'] = 'Value4:'

    def toggle_buttons(self, state):
        state_string = 'normal' if state else 'disabled'
        self.button_copy_value1['state'] = state_string
        self.button_copy_value2['state'] = state_string
        self.button_copy_value3['state'] = state_string
        self.button_copy_value4['state'] = state_string

    def copy_value1(self):
        copy(self.var_value1.get())

    def copy_value2(self):
        copy(self.var_value2.get())

    def copy_value3(self):
        copy(self.var_value3.get())

    def copy_value4(self):
        copy(self.var_value4.get())


class WindowFavorites(WindowButtonBase):

    def __init__(self, button):
        if Image is None:
            install_pillow()
            raise Exception()
        super().__init__(button)
        self.resizable(False, False)
        self.title('Favorites')
        self.geometry('400x168')
        bytes_io = BytesIO(IMAGE_SHOP)
        self.image_items = Image.open(bytes_io)
        self.label_name = Label(self, text='Name:', font=BUTTON_FONT)
        self.label_name.place(x=10, y=10)
        self.var_name = StringVar(self)
        self.entry_name = Entry(self, font=BUTTON_FONT,
                                textvariable=self.var_name)
        self.entry_name.bind('<Return>', self.query)
        self.entry_name.place(x=80, y=10, width=220, height=30)
        self.button_query = Button(
            self, text='Query', font=BUTTON_FONT, command=self.query)
        self.button_query.place(x=310, y=10, width=80, height=30)
        self.image_shop = Image.new('RGB', (252, 109))
        self.photo_shop = ImageTk.PhotoImage(self.image_shop)
        self.label_shop = Label(self, image=self.photo_shop)
        self.label_shop.place(x=74, y=50, width=252, height=108)
        self.generate('null,' * 21)
        self.focus_force()
        self.after(1, lambda: self.entry_name.focus_force())

    def generate(self, string):
        favorites = string.split(',')[:21]
        for i, favorite in enumerate(favorites):
            x = i % 7
            y = i // 7
            index = MAP_SHOP.get(favorite, 0)
            image_item = self.image_items.crop(
                (0, index * 36, 36, index * 36 + 36))
            self.image_shop.paste(
                image_item, (x * 36, y * 36, x * 36 + 36, y * 36 + 36))
        self.photo_shop = ImageTk.PhotoImage(self.image_shop)
        self.label_shop['image'] = self.photo_shop

    def query(self, event=None):
        self.title('Favorites (Querying...)')
        try:
            username = self.var_name.get().strip()
            if RE_USERNAME.match(username):
                json_profile = request_json(
                    'api.mojang.com', f'/users/profiles/minecraft/{username}')
                uuid = json_profile['id']
                json_hypixel = request_json(
                    'api.hypixel.net', f'/player?key=aeddc0f8-e404-4216-b0e9-e7d68d859258&uuid={uuid}')
                favorites = json_hypixel['player']['stats']['Bedwars']['favourites_2']
                self.generate(favorites)
                self.title('Favorites')
            else:
                self.generate('null,' * 21)
                self.title('Favorites (Failed)')
        except Exception:
            self.generate('null,' * 21)
            self.title('Favorites (Failed)')


