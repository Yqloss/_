def request_bytes_http(host, url):
    connection = HTTPConnection(host=host)
    connection.request(method='GET', url=url)
    response = connection.getresponse()
    return response.read()


def request_bytes(host, path):
    connection = HTTPSConnection(host=host, context=SSL_CONTEXT)
    connection.request(method='GET', url=f'https://{host}{path}')
    response = connection.getresponse()
    return response.read()


def request_json(host, path):
    return json.loads(request_bytes(host, path).decode())


def request_banner(username):
    connection = HTTPSConnection(host='optifine.net', context=SSL_CONTEXT)
    connection.request(method='POST', url='https://optifine.net/banners',
                       body=f'username={username}'.encode(), headers={'Content-Type': 'application/x-www-form-urlencoded'})
    response = connection.getresponse()
    location = response.getheader('Location')
    format = location.split('=')[1].split('&')[0]
    return format


class WindowButtonBase(Toplevel):

    def __init__(self, button):
        super().__init__()
        self.button = button
        self.protocol('WM_DELETE_WINDOW', self.button.on_window_closed)


class ButtonWindow(Button):

    def __init__(self, root, text, window_class):
        super().__init__(root, text=text, font=BUTTON_FONT, command=self.on_click)
        self.window_class = window_class
        self.window = None
        self.window_thread = None

    def on_click(self):
        if self.window == None:
            try:
                self.window = self.window_class(self)
                self.window.mainloop()
            except Exception:
                self.window = None
        else:
            self.window.focus_force()

    def on_window_closed(self):
        self.window.destroy()
        self.window = None


