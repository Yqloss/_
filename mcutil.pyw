    def query(self, event=None):
        self.title('Account (Querying...)')
        self.toggle_buttons(False)
        self.clear_vars()
        try:
            username = self.var_name.get().strip()
            if RE_USERNAME.match(username):
                json_profile = request_json(
                    'api.mojang.com', f'/users/profiles/minecraft/{username}')
                uuid = json_profile['id']
                json_session = request_json(
                    'sessionserver.mojang.com', f'/session/minecraft/profile/{uuid}')
                textures_base64 = json_session['properties']
                for property_ in textures_base64:
                    if property_['name'] == 'textures':
                        textures_base64 = property_['value']
                        break
                textures = json.loads(base64.b64decode(
                    textures_base64.encode()).decode())
                skin_url = textures['textures']['SKIN']['url']
                model = 'default'
                try:
                    model = textures['textures']['SKIN']['metadata']['model']
                except Exception:
                    model = 'default'
                cape_url = 'None'
                try:
                    cape_url = textures['textures']['CAPE']['url']
                except Exception:
                    cape_url = 'None'
                self.var_uuid.set(uuid)
                self.var_skin.set(skin_url)
                self.var_model.set(model)
                self.var_cape.set(cape_url)
                self.title('Account')
                self.toggle_buttons(True)
                if cape_url == 'None':
                    self.button_save_cape['state'] = 'disabled'
            else:
                self.var_uuid.set('Invalid name')
                self.title('Account (Failed)')
        except Exception:
            self.var_uuid.set('Not exist')
            self.title('Account (Failed)')

    def toggle_buttons(self, state):
        state_string = 'normal' if state else 'disabled'
        self.button_copy_uuid['state'] = state_string
        self.button_save_skin['state'] = state_string
        self.button_save_cape['state'] = state_string

    def clear_vars(self):
        self.var_uuid.set('')
        self.var_skin.set('')
        self.var_model.set('')
        self.var_cape.set('')

    def copy_uuid(self):
        copy(self.var_uuid.get())

    def save_file(self, host, url, initialfile):
        self.title('Account (Saving...)')
        try:
            filename = asksaveasfilename(
                parent=self, defaultextension='.png', initialfile=initialfile)
            if filename != '':
                with open(filename, 'wb') as fw:
                    content = request_bytes_http(host, url)
                    fw.write(content)
            self.title('Account')
        except Exception:
            self.title('Account (Failed)')

    def save_skin(self):
        self.save_file('textures.minecraft.net', self.var_skin.get(
        ), f'skin_{self.var_model.get()}_{self.var_name.get()}.png')

    def save_cape(self):
        self.save_file('textures.minecraft.net',
                       self.var_cape.get(), f'cape_{self.var_name.get()}.png')


